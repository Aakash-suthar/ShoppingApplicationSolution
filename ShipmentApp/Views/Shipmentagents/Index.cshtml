@model IEnumerable<ShipmentApp.Models.Shipmentagent>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Orderid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Orderplacedate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Deliverydate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryGuy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Statuss)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Orderid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Orderplacedate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deliverydate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryGuy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Statuss)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
